					"size": "1rem",
					"slug": "text-xs"
				},
				{
					"name": "S",
					"size": "1.125rem",
					"slug": "text-s"
				},
				{
					"name": "M",
					"size": "clamp(1.125rem, 1.08333rem + 0.208333vw, 1.25rem)",
					"slug": "text-m"
				},
				{
					"name": "L",
					"size": "clamp(1.25rem, 1.16667rem + 0.416667vw, 1.5rem)",
