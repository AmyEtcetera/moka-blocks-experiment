				}
			],
			"customFontSize": true
		},
		"custom": {
			"line-height": {
				"body": 1.4,
				"heading": 1
			},
			"body": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lato)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lato)"
				}
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": null
		},
		"color": {
			"background": "var(--wp--custom--color--main-background)",
			"text": "var(--wp--preset--color--font-secondary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-xxxl)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-xxl)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-xl)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-l)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-m)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-s)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--custom--body--typography--font-family)",
			"lineHeight": "var(--wp--custom--line-height--body)",
			"fontWeight": "400"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"lineHeight": "var(--wp--custom--line-height--heading)",
					"fontWeight": "500"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"lineHeight": "var(--wp--custom--line-height--heading)",
					"fontWeight": "500"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"fontWeight": "400"
				}
			},
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--button-color-primary)"
				}
			},
			"core/list": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-m)"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--custom--body--typography--font-family)",
					"fontSize": "var(--wp--preset--font-size--text-m)"
				}
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--code-background)",
					"text": "var(--wp--preset--color--code-color)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--text-s)"
				}
			}
		}
